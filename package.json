{
    "name": "ripe-skeleton-vue",
    "version": "0.1.0",
    "description": "RIPE Skeleton for Vue.js",
    "keywords": [
        "components",
        "skeleton",
        "ripe"
    ],
    "homepage": "https://github.com/ripe-tech/ripe-skeleton-vue#readme",
    "bugs": {
        "url": "https://github.com/ripe-tech/ripe-skeleton-vue/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ripe-tech/ripe-skeleton-vue.git"
    },
    "license": "Apache-2.0",
    "author": "Platforme",
    "main": "js/main.js",
    "files": [
        "index.html",
        "css/**/*.css",
        "js/**/*.js",
        "res/**/*.ico",
        "res/**/*.png",
        "test/**/*.js",
        "vue/**/*.js",
        "vue/**/*.vue"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --mode production --config webpack.config.js",
        "build-dev": "cross-env NODE_ENV=development webpack --mode development --config webpack.config.js",
        "dev": "cross-env NODE_ENV=development webpack serve --config webpack.config.js --mode development --hot",
        "lint": "eslint \"./**/*.{js,vue}\"",
        "lint-all": "npm run lint && npm run stylelint",
        "lint-fix": "eslint \"./**/*.{js,vue}\" --fix",
        "prettier": "prettier \"./**/*.{js,vue,json}\" --write",
        "pretty": "npm run prettier && npm run lint-fix && npm run stylelint-fix && npm run svgo",
        "stylelint": "stylelint \"./**/*.{css,scss,vue}\"",
        "stylelint-fix": "stylelint \"./**/*.{css,scss,vue}\" --fix",
        "svgo": "svgo -r .",
        "test": "mocha --recursive",
        "upgrade": "npx sort-package-json && ncu -u",
        "webpack": "webpack --config webpack.config.js"
    },
    "dependencies": {
        "@vue/compat": "^3.2.31",
        "@vue/compiler-sfc": "^3.2.31",
        "ripe-components-vue": "^0.18.1",
        "ripe-id-api": "^0.6.1",
        "ripe-sdk": "^2.19.0",
        "vue": "^3.2.31",
        "vue-global-events": "^2.1.0",
        "vue-router": "^4.0.12"
    },
    "devDependencies": {
        "@babel/core": "^7.17.2",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.16.11",
        "@babel/runtime": "^7.17.2",
        "babel-loader": "^8.2.3",
        "core-js": "^3.21.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.6.0",
        "eslint": "^8.8.0",
        "eslint-config-hive": "^0.5.3",
        "eslint-webpack-plugin": "^3.1.1",
        "fibers": "^5.0.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "mocha": "^9.2.0",
        "npm-check-updates": "^12.3.0",
        "postcss": "^8.4.6",
        "postcss-html": "^1.3.0",
        "prettier": "^2.5.1",
        "prettier-config-hive": "^0.1.7",
        "sass": "^1.49.7",
        "sass-loader": "^12.4.0",
        "sort-package-json": "^1.54.0",
        "style-loader": "^3.3.1",
        "stylelint": "^14.5.0",
        "stylelint-config-hive": "^1.0.0",
        "stylelint-config-html": "^1.0.0",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-config-standard-scss": "^3.0.0",
        "stylelint-order": "^5.0.0",
        "svgo": "^2.8.0",
        "vue-loader": "^17.0.0",
        "webpack": "^5.68.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4"
    }
}
