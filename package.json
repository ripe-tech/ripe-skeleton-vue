{
    "name": "ripe-skeleton-vue",
    "version": "0.1.0",
    "description": "RIPE Skeleton for Vue.js",
    "keywords": [
        "components",
        "skeleton",
        "ripe"
    ],
    "homepage": "https://github.com/ripe-tech/ripe-skeleton-vue#readme",
    "bugs": {
        "url": "https://github.com/ripe-tech/ripe-skeleton-vue/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ripe-tech/ripe-skeleton-vue.git"
    },
    "license": "Apache-2.0",
    "author": "Platforme",
    "files": [
        "index.html",
        "css/**/*.css",
        "js/**/*.js",
        "res/**/*.ico",
        "res/**/*.png",
        "test/**/*.js",
        "vue/**/*.js",
        "vue/**/*.vue"
    ],
    "main": "js/main.js",
    "scripts": {
        "build": "webpack --mode production --config webpack.config.js",
        "build-dev": "webpack --mode development --config webpack.config.js",
        "dev": "webpack-dev-server --config webpack.config.js --mode development --watch --hot",
        "lint": "eslint \"./**/*.{js,vue}\"",
        "lint-all": "npm run lint && npm run stylelint",
        "lint-fix": "eslint \"./**/*.{js,vue}\" --fix",
        "prettier": "prettier \"./**/*.{js,vue,json}\" --write",
        "pretty": "npm run prettier && npm run lint-fix && npm run stylelint-fix",
        "stylelint": "stylelint \"./**/*.{css,scss,vue}\"",
        "stylelint-fix": "stylelint \"./**/*.{css,scss,vue}\" --fix",
        "test": "mocha --recursive",
        "upgrade": "npx sort-package-json && ncu -u",
        "webpack": "webpack --config webpack.config.js"
    },
    "dependencies": {
        "ripe-components-vue": "^0.2.2",
        "ripe-id-api": "^0.1.11",
        "ripe-sdk": "^1.6.7",
        "vue": "^2.6.10",
        "vue-global-events": "^1.1.2",
        "vue-router": "^3.0.6",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/polyfill": "^7.4.4",
        "uxf-webpack": "^0.2.11"
    }
}
